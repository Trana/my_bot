<?xml version="1.0" ?>
<sdf version="1.8">
    <model name='diff_drive'>


      <!-- Placed at center of wheels -->
      <link name="base_link">
      </link>
      
      <joint name="chassis_joint" type="fixed">
        <parent>base_link</parent>
        <child>chassis</child>
        <pose relative_to='base_link'>-0.05 0 -0.0205 0 0 0</pose>
      </joint>

      <link name='chassis'>
        <pose relative_to='chassis_joint'>0.1175 0 0.045 0 0 0</pose>
        <inertial>
          <mass>10</mass>
          <inertia>
            <ixx>0.126164</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.416519</iyy>
            <iyz>0</iyz>
            <izz>0.481014</izz>
          </inertia>
        </inertial>
        <visual name='visual_chassis'>
          <geometry>
            <box>
              <!-- Length width height -->
              <size>0.235 0.235 0.09</size>
            </box>
          </geometry>
          <material>
            <ambient>0 0 0 1</ambient>
            <diffuse>0 0 0 1</diffuse>
            <specular>0.0 0.0 0.0 1</specular>
          </material>
        </visual>
        <collision name='collision_chassis'>
          <geometry>
            <box>
              <size>0.235 0.235 0.009</size>
            </box>
          </geometry>
        </collision>
      </link>

      <!-- Left Wheel -->
      <joint name='left_wheel_joint' type='revolute'>
        <parent>base_link</parent>
        <child>left_wheel</child> 
        <!-- Wheel is 14 cm out from center of base link
        so that sphere is not flat it is turned by pi/2 rad -->
        <pose relative_to="base_link">0 0.14 0 0 0 0</pose>
        
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

       <link name='left_wheel'>
       <pose relative_to="left_wheel_joint">0 0 0 1.5707 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.145833</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.145833</iyy>
            <iyz>0</iyz>
            <izz>0.125</izz>
          </inertia>
        </inertial>
        <visual name='visual_left_wheel'>
          <geometry> 
            <cylinder>
              <!-- 6.5 cmd diameter and 3 cm thick -->
              <radius>0.0325</radius>
              <length>0.03</length>
            </cylinder> 
                         <!-- <sphere>
              <radius>0.0325</radius>
            </sphere> -->
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision_left_wheel'>
          <geometry>
            <cylinder>
              <!-- 6.5 cmd diameter and 3 cm thick -->
              <radius>0.0325</radius>
              <length>0.03</length>
            </cylinder> 
          </geometry>
        </collision>
      </link>

      <!-- Right wheel -->
      <joint name='right_wheel_joint' type='revolute'>
        <parent>base_link</parent>
        <child>right_wheel</child> 
        <!-- Wheel is 14 cm out from center of base link
        so that sphere is not flat it is turned by pi/2 rad -->
      <pose relative_to="base_link">0 -0.14 0 0 0 0</pose>
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>
      
      <link name='right_wheel'>
      <pose relative_to="right_wheel_joint">0 0 0 -1.5707 0 0</pose>
       <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.145833</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.145833</iyy>
            <iyz>0</iyz>
            <izz>0.125</izz>
          </inertia>
        </inertial>
        <visual name='visual_left_wheel'>
          <geometry>
            <cylinder>
              <!-- 6.5 cmd diameter and 3 cm thick -->
              <radius>0.0325</radius>
              <length>0.03</length>
            </cylinder>
           
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision_left_wheel'>
          <geometry>
             <cylinder>
              <!-- 6.5 cmd diameter and 3 cm thick -->
              <radius>0.0325</radius>
              <length>0.03</length>
            </cylinder>
          </geometry>
        </collision>
      </link>


      <joint name='caster_wheel_joint' type='ball'>
        <parent>chassis</parent>
        <child>caster_wheel</child>
      </joint>

      <link name='caster_wheel'>
      <!-- 9 cm forward from center of chassis and half height of chassis plus half heigh of caster wheel 1 cm -->
        <pose relative_to="chassis"> 0.09 0 -0.046 0 0 0</pose>
        <inertial>
          <mass>1</mass>
          <inertia>
            <ixx>0.1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1</iyy>
            <iyz>0</iyz>
            <izz>0.1</izz>
          </inertia>
        </inertial>
        <visual name='visual_caster'>
          <geometry>
            <sphere>
              <radius>0.01</radius>
            </sphere>
          </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision_caster'>
          <geometry>
            <sphere>
              <radius>0.01</radius>
            </sphere>
          </geometry>
        </collision>
      </link>

      <!-- LIDAR -->
      <joint name='lidar_joint' type='fixed'>
        <parent>chassis</parent>
        <child>lidar_link</child>
      </joint> 

      <link name="lidar_link">
        <pose relative_to="chassis">0 0 0.045 0 0 0</pose>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.000166667</ixx>
            <iyy>0.000166667</iyy>
            <izz>0.000166667</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.07 0.07 0.05</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.07 0.07 0.05</size>
            </box>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
            <specular>0.0 0.0 1 1</specular>
          </material>
        </visual>
        <sensor name='gpu_lidar' type='gpu_lidar'>
          <pose>0 0 0 0 0 0</pose>
          <topic>scan</topic>
          <ignition_frame_id>diff_drive/lidar_link</ignition_frame_id>
          <update_rate>10</update_rate>
          <lidar>
            <scan>
              <horizontal>
                <samples>640</samples>
                <resolution>1</resolution>
                <min_angle>-3.14</min_angle>
                <max_angle>3.14</max_angle>
              </horizontal>
              <vertical>
                <samples>1</samples>
                <resolution>1</resolution>
                <min_angle>0.0</min_angle>
                <max_angle>0.0</max_angle>
              </vertical>
            </scan>
            <range>
              <min>0.08</min>
              <max>10.0</max>
              <resolution>0.01</resolution>
            </range>
          </lidar>
          <visualize>true</visualize>
        </sensor>
      </link>

      <!-- CAMERA -->
      <joint name='camera_joint' type='fixed'>
        <parent>chassis</parent>
        <child>camera_link</child>
      </joint>

      <link name="camera_link">
        <pose relative_to="chassis">0.1175 0 0 0 0 0</pose>       
        <visual name="visual">
          <geometry>
            <box>
              <size>0.025 0.025 0.025</size>
            </box>
          </geometry>
          <material>
            <ambient>0 0 1 1</ambient>
            <diffuse>0 0 1 1</diffuse>
            <specular>0.0 0.0 1 1</specular>
          </material>
        </visual>
        <sensor name='camera' type='camera'>
          <pose>0 0 0 0 0 0</pose>
          <ignition_frame_id>diff_drive/camera_link</ignition_frame_id>
          <visualize>true</visualize>
          <update_rate>10</update_rate>
          <topic>image</topic>
          <camera>
            <always_on>1</always_on>
            <horizontal_fov>1.089</horizontal_fov>
            <image>
              <format>R8G8B8</format>
              <width>640</width>
              <height>480</height>
            </image>
          </camera>
          <clip>
            <near>0.05</near>
            <far>8.0</far>
          </clip>
        </sensor>
      </link>

      <joint name='camera_optical_joint' type='fixed'>
        <parent>camera_link</parent>
        <child>camera_link_optical</child>        
      </joint>

      <link name="camera_link_optical">
        <pose>0 0 0 -1.57079632679 0 -1.57079632679</pose>
      </link>
      
      <ros2_control name="IgnitionSystem" type="system">
        <hardware>
          <plugin>ign_ros2_control/IgnitionSystem</plugin>
        </hardware>
        <joint name="left_wheel_joint">
          <command_interface name="velocity">
            <param name="min">-10</param>
            <param name="max">10</param>
          </command_interface>
          <state_interface name="position"/>
          <state_interface name="velocity"/>
        </joint>
        <joint name="right_wheel_joint">
          <command_interface name="velocity">
            <param name="min">-10</param>
            <param name="max">10</param>
          </command_interface>
          <state_interface name="position"/>
          <state_interface name="velocity"/>
        </joint>
      </ros2_control>
      
      <plugin filename="ign_ros2_control-system" name="ign_ros2_control::IgnitionROS2ControlPlugin">
            <parameters>/home/mikaeltrana/Documents/robot/my_bot/install/ros_gz_example_bringup/share/ros_gz_example_bringup/config/my_controller.yaml</parameters>
      </plugin>
          
